<?xml version="1.0" encoding="UTF-8"?>
<project name="JWS.makejar" default="makejar" basedir=".">
	<target name="makejar" description="Create a jar for the JWS">
		<jar destfile="JWSCards.jar" includes="**/*.class" basedir="bin">
			<manifest>
				<attribute name="Main-Class" value="com.alex.cards.StartFrame" />
				<attribute name="Permissions" value="sandbox" />
				<attribute name="Codebase" value="tspigot.student.umd.edu" />
			</manifest>
		</jar>
	</target>
</project>
